  "configurations": [
    {
      "id": "olmed-sync-products",
      "name": "Synchronizacja produktów Olmed",
      "description": "Pobieranie produktów z API Olmed co 2 godziny - synchronizacja produktów dla marketplace APTEKA_OLMED",
      "isActive": true,
      "intervalSeconds": 7200,
      "method": "POST",
      "url": "https://draft-csm-connector.grupaolmed.pl/erp-api/products/get-products",
      "useOlmedAuth": true,
      "headers": {
        "accept": "application/json",
        "Content-Type": "application/json",
        "X-CSRF-TOKEN": ""
      },
      "body": "{\"marketplace\": \"APTEKA_OLMED\"}",
      "marketplace": "APTEKA_OLMED",
      "additionalParameters": {
        "priority": "high",
        "retryCount": 3,
        "timeout": 30
      }
    },
    {
      "id": "olmed-sync-orders",
