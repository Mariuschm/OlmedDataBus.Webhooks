  "configurations": [
    {
      "id": "olmed-sync-products",
      "name": "Synchronizacja produktów Olmed",
      "description": "Pobieranie produktów z API Olmed co 10 minut - synchronizacja produktów dla marketplace APTEKA_OLMED",
      "isActive": true,
      "intervalSeconds": 600,
      "method": "POST",
      "url": "https://draft-csm-connector.grupaolmed.pl/erp-api/products/get-products",
      "useOlmedAuth": true,
      "headers": {
        "accept": "application/json",
        "Content-Type": "application/json",
        "X-CSRF-TOKEN": ""
      },
      "body": "{\"marketplace\": \"APTEKA_OLMED\",   \"skus\": [\"1787\"] }",
      "marketplace": "APTEKA_OLMED",
      "additionalParameters": {
        "priority": "high",
        "retryCount": 3,
        "timeout": 30
      }
    }
  ]
}