      "useOlmedAuth": true,
      "headers": {
        "accept": "application/json",
        "Content-Type": "application/json",
        "X-CSRF-TOKEN": ""
      },
      "body": "{\"marketplace\": \"APTEKA_OLMED\", \"status\": \"new\"}",
      "marketplace": "APTEKA_OLMED",
      "additionalParameters": {
        "priority": "high",
